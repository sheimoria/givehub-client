    "react-datepicker": "latest",
    "react-dom": "latest",
    "react-hook-form": "latest",
    "tailwindcss-invalid-variant-plugin": "latest",
    "yup": "latest"
  },
  "devDependencies": {
    "@types/graphql": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "apollo": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
